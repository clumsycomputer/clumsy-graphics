{
  "name": "@clumsycomputer/graphics-renderer",
  "version": "2.0.0",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@types/express": "^4.17.13",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "axios": "^0.27.2",
    "esbuild": "^0.13.11",
    "esbuild-node-externals": "^1.3.0",
    "express": "^4.17.1",
    "fp-ts": "^2.11.1",
    "inkscape": "^3.1.1",
    "io-ts": "^2.2.16",
    "isomorphic-fetch": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "typescript": "4.4.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.16.3"
  },
  "scripts": {
    "test": "yarn playwright test --quiet",
    "buildDist": "rm -rf ./dist && tsc --project ./tsconfig.dist.json --outDir ./dist && cp -r ./source/startAnimationDevelopment/browser ./dist/startAnimationDevelopment/browser",
    "buildLocal": "yarn buildDist && chmod +x ./dist/main.js && yarn link",
    "startExampleDevelopment": "graphics-renderer startDevelopment --animationModulePath=./example-project/Foo.animation.tsx",
    "renderExampleAnimation": "graphics-renderer renderAnimation --animationModulePath=./example-project/Foo.animation.tsx --animationMp4OutputPath=./example-project/example.mp4",
    "convertExampleAnimationToGif": "graphics-renderer convertAnimationToGif --animationMp4SourcePath=./example-project/foo.mp4 --animationGifOutputPath=./example-project/foo.gif --gifAspectRatioWidth=512"
  },
  "bin": {
    "graphics-renderer": "./dist/main.js"
  }
}
