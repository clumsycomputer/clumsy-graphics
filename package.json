{
  "name": "@clumsycomputer/graphics-renderer",
  "version": "2.0.4-alpha.1",
  "main": "dist/docker-main.js",
  "types": "dist/docker-main.d.ts",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@types/express": "^4.17.13",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "esbuild": "^0.13.11",
    "esbuild-node-externals": "^1.3.0",
    "express": "^4.17.1",
    "fp-ts": "^2.11.1",
    "inkscape": "^3.1.1",
    "io-ts": "^2.2.16",
    "isomorphic-fetch": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "typescript": "4.4.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2"
  },
  "scripts": {
    "test": "playwright test --quiet",
    "buildDist": "rm -rf ./dist && tsc --project ./tsconfig.dist.json --outDir ./dist && cp -r ./source/startAnimationDevelopment/browser ./dist/startAnimationDevelopment/browser && cp ./dist.docker-compose.yml ./dist/dist.docker-compose.yml && cp ./dist.Dockerfile ./dist/dist.Dockerfile",
    "buildLocal": "yarn buildDist && chmod +x ./dist/docker-main.js && chmod +x ./dist/native-main.js && yarn link",
    "startDevelopmentEnvironment": "docker compose --file ./dev.docker-compose.yml build && docker compose --file ./dev.docker-compose.yml run --rm development-environment",
    "startExampleDevelopment": "_graphics-renderer startDevelopment --animationModulePath=./example-project/Foo.animation.tsx",
    "renderExampleAnimation": "_graphics-renderer renderAnimation --animationModulePath=./example-project/Foo.animation.tsx --animationMp4OutputPath=./example-project/example.mp4",
    "convertExampleAnimationToGif": "_graphics-renderer convertAnimationToGif --animationMp4SourcePath=./example-project/foo.mp4 --animationGifOutputPath=./example-project/foo.gif --gifAspectRatioWidth=512"
  },
  "bin": {
    "_graphics-renderer": "./dist/docker-main.js",
    "graphics-renderer": "./dist/native-main.js"
  }
}
